{
  "name": "leaflet-tiny-log-collector",
  "version": "1.0.0-dev",
  "bin": "./dist/tlc-main.js",
  "scripts": {
    "start": "ts-node ./src/tlc-main.ts",
    "diststart": "node ./dist/src/tlc-main.js",
