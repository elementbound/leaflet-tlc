    "config": "3.3.9",
    "js-yaml": "4.1.0",
    "jsonpath-plus": "7.2.0",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@babel/core": "7.20.2",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-typescript": "7.18.6",
    "@types/config": "3.3.0",
    "@types/jest": "29.2.3",
    "@types/node": "18.11.9",
    "@types/sinon": "10.0.13",
    "jest": "29.3.1",
    "prettier": "2.8.8",
    "sinon": "14.0.2",
    "tsconfig-paths": "4.2.0",
    "tsc-alias": "1.8.6",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "typescript": "4.8.4"
  }
